{
    "builds": [{
        "src": "fenil_project/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" , "runtime":"python3.11" }
    },
    {
        "src":"build_file.sh",
        "use": "@vercel/static-build",
        "config":{
            "distDir":"staticfile_build"
        }
    }
], "routes": [
    
    {
        "src": "/(.*)",
        "dest": "final_project/wsgi.py"
    },{
        "src": "/static/(.*)",
        "dest": "/static/$1"
    }

]
    
}